{
    "name": "ramanandapanda/jsksms",
    "description": "jsksms Notifications channel for Laravel >= 5.5, 6 and  8.",
    "keywords": ["laravel", "notifications", "jsksms", "message", "sms"],
    "homepage": "https://github.com/ramanandapanda/jsksms",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramananda Panda",
            "email": "ramanandapanda@hotmail.com",
            "homepage": "https://github.com/ramanandapanda",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4 || ^7.0",
        "illuminate/notifications": "5.5 - 5.8 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "5.5 - 5.8 || ~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\jsksms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\jsksms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "NotificationChannels\\jsksms\\JsksmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
